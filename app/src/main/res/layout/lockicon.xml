<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="162dp"
    android:layout_height="140dp"
    android:padding="0dp">

    <ImageView
        android:id="@+id/lockicon"
        android:layout_width="53dp"
        android:layout_height="78dp"
        android:src="@drawable/ic_baseline_lock_24"
        app:layout_constraintBottom_toBottomOf="@id/circlelogo"
        app:layout_constraintEnd_toEndOf="@id/circlelogo"
        app:layout_constraintStart_toStartOf="@id/circlelogo"
        app:layout_constraintTop_toTopOf="@id/circlelogo" />

    <ImageView
        android:id="@+id/circlelogo"
        android:layout_width="153dp"
        android:layout_height="139dp"
        android:layout_marginEnd="4dp"
        android:src="@drawable/ic_baseline_radio_button_unchecked_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>